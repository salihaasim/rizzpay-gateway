
# Server Configuration
PORT=3001
NODE_ENV=development

# Database
SUPABASE_URL=your_supabase_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Security
JWT_SECRET=your_jwt_secret_here
API_KEY_PREFIX=rizz_

# AES Encryption Configuration
AES_ENCRYPTION_KEY=your_master_aes_encryption_key_here_minimum_32_chars

# Bank-Specific AES Keys (for compliance)
CANARA_BANK_AES_KEY=your_canara_bank_specific_aes_key_here_32_chars
HDFC_BANK_AES_KEY=your_hdfc_bank_specific_aes_key_here_32_chars
SBM_BANK_AES_KEY=your_sbm_bank_specific_aes_key_here_32_chars
ICICI_BANK_AES_KEY=your_icici_bank_specific_aes_key_here_32_chars

# Bank API Configuration
HDFC_API_URL=https://api.hdfcbank.com
HDFC_API_KEY=your_hdfc_api_key_here
SBM_API_URL=https://api.sbmbank.com
SBM_API_KEY=your_sbm_api_key_here
CANARA_API_URL=https://api.canarabank.com
CANARA_API_KEY=your_canara_api_key_here

# CORS
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173

# Webhook URLs
WEBHOOK_BASE_URL=https://your-domain.com/api/webhooks

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info
