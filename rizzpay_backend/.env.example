
# Server Configuration
PORT=3001
NODE_ENV=development

# Database
SUPABASE_URL=your_supabase_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Security
JWT_SECRET=your_jwt_secret_here
API_KEY_PREFIX=rizz_

# CORS
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173

# Bank API Configuration
HDFC_API_URL=https://api.hdfcbank.com
HDFC_API_KEY=your_hdfc_api_key_here
SBM_API_URL=https://api.sbmbank.com
SBM_API_KEY=your_sbm_api_key_here

# Webhook URLs
WEBHOOK_BASE_URL=https://your-domain.com/api/webhooks

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info
